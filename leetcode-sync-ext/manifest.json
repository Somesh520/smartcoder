{
  "manifest_version": 3,
  "name": "LeetCode Connector Universal",
  "version": "2.1",
  "description": "Syncs LeetCode to ANY Localhost port.",
  "permissions": [
    "cookies",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/*",
    "http://localhost:*/*",
    "http://127.0.0.1:*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}